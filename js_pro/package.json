{
  "name": "ollama-proxy",
  "version": "1.0.0",
  "description": "Ollama-compatible API proxy using Vercel AI SDK",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "bun --watch src/main.ts",
    "start": "bun src/main.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ollama",
    "ai",
    "proxy",
    "vercel-ai-sdk"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/azure": "^1.0.0",
    "@ai-sdk/cohere": "^1.0.0",
    "@ai-sdk/deepseek": "^1.0.0",
    "@ai-sdk/fireworks": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/groq": "^1.0.0",
    "@ai-sdk/mistral": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "@ai-sdk/togetherai": "^1.0.0",
    "@ai-sdk/xai": "^1.0.0",
    "ai": "^4.0.0",
    "chokidar": "^4.0.0",
    "hono": "^4.6.0",
    "i18next": "^25.8.4",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/bun": "^1.3.8",
    "@types/node": "^22.0.0",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
